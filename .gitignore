.DS_Store
# Environment files
.localenvs/


# Binary output directory
bin/

# Binary files (never commit these)
# Main binaries - NEVER COMMIT BINARIES
unified
cmd/unified/unified
cmd/aggregator/aggregator
cmd/dequeuer/dequeuer
cmd/finalizer/finalizer
cmd/p2p-gateway/p2p-gateway
cmd/state-tracker/state-tracker
cmd/vpa-relayer/vpa-relayer
cmd/epoch-manager/epoch_generator
cmd/monitor-api/monitor-api

# Any file without extension in cmd directories (but allow .go, .md, Dockerfile*)
# First, explicitly allow source files and docs
!cmd/*/*.go
!cmd/*/*.md
!cmd/*/Dockerfile*
# Then ignore binaries (files without extension) in cmd directories
cmd/*/[!.]*
!cmd/*/

# Specific binary names (catch-all)
vpa-relayer
aggregator
dequeuer
finalizer
p2p-gateway
state-tracker
epoch_generator
monitor-api
unified-sequencer

# Test binaries
*.test

# Build artifacts
*.exe
*.dll
*.so
*.dylib
